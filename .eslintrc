{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true,
  },
  "rules": {
    "semi": [0],
    "comma-dangle": [0],
    "global-require": [0],
    "no-alert": [0],
    "no-console": [0],
    "no-param-reassign": [0],
    "max-len": [0],
    "func-names": [0],
    "no-underscore-dangle": [0],
    "object-shorthand": [1],
    "arrow-body-style": [1, "as-needed"],
    "no-new": [0],
    "strict": [0],
    "spaced-comment": [0],
    "no-unused-vars": [0],
    "no-empty": [0],
    "no-constant-condition": [0],
    "no-else-return": [0],
    "no-use-before-define": [0],
    "no-unused-expressions": [0],
    "react/wrap-multilines": [0],
    "react/no-multi-comp": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "react/prefer-stateless-function": [0],
    "react/sort-comp": [0]
  },
  "globals": {
    "_": true
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
}
